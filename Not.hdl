/**
 * Not gate:
 * if (in) out = 0, else out = 1
 */
CHIP Not {
    IN in;
    OUT out;

    PARTS:
    // Not using Nand Gate
    // (a NAND a) = NOT(a)
    Nand(a = in, b= in,out= out );
}
